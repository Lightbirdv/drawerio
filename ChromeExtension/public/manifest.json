{
  "name": "Drawer.io",
  "description": "Chrome Extension for grabbing Content on Websites.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "./images/128.png",
    "48": "./images/48.png",
    "16": "./images/16.png"
  },
  "action": {
    "default_icon": "./images/16.png",
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["https://*/", "http://*/"]
}
